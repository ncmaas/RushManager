<% title "Users" %>

<table id="table_users" class="datatable table table-striped table-bordered">
  <thead>
  <tr>
    <th>NetID</th>
    <th>First Name</th>
    <th>Nickname</th>
    <th>Last Name</th>
  </tr>
  </thead>
  <tbody>
  <% for user in @users %>
    <tr>
    <td><%= user.login %></td>
    <td><%= link_to user.first_name, user %></td>
    <td><%= link_to user.nick_name, user %></td>
    <td><%= link_to user.last_name, user %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<hr />
<p> 
  <%= link_to "New User", new_user_path, :class => "btn btn-primary" %>
</p>
